import TaelinArena.Game
import TaelinArena.Models

teichi_hero : Hero
  let stat = stat(120, 2, 10)
  let inpt = (this, input) => this
  let tick = (this) => this
  let draw = (this) =>
    case this |thing
    let model = aniloop(TEICHI_ATK_R_0, 10, 3, this.tik)
    draw(model, this.pos, this.dir)
  hero(stat, inpt, tick, draw)
